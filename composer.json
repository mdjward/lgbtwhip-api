{
    "name": "TheLGBTWhip API",
    "version": "2.0.0",
    "require": {
        "slim/slim": "2.*",
        "doctrine/orm": "*",
        "doctrine/migrations": "1.0.*@dev",
        "symfony/dependency-injection": "2.*",
        "symfony/config": "2.*",
        "symfony/yaml": "2.*",
        "symfony/expression-language": "2.*",
        "jms/serializer": "0.16.*",
        "icanboogie/inflector": "1.2",
        "guzzlehttp/guzzle": "~5.2"
    },
    "require-dev": {
        "hafriedlander/phockito": "~1.0",
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-4": {
            "TheLgbtWhip\\Api\\Tests\\": "src/TheLgbtWhip/Api/Tests/",
            "TheLgbtWhip\\Api\\": "src/TheLgbtWhip/Api/"
        }
    },
    "config": {
        "vendor-dir": "lib"
    }
}
